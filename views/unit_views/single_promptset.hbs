{{#*inline "title"}}Prompt Set | Twennie{{/inline}}
<div class="view-promptset-container">
  <!-- Left Section -->
  <div class="promptset-view-left">
  
    <!-- Clock Icon -->
<div class="view-promptset-clock" style="text-align: center; margin-bottom: 1rem;">
  <img src="/images/20mins.svg" alt="Estimated time to complete: 20 minutes" style="width: 100px; height: auto;">
</div>


    <h1 class="view-promptset-title">{{promptset_title}}</h1>
    <p class="view-promptset-description">{{short_summary}}</p>

    <!-- Starting Message (Prompt 0) -->
    <div class="view-promptset-starting-container">
      <h2 class="view-promptset-starting-title">Why should I register for this prompt set?</h2>
      <h2 class="view-promptset-starting-title2">{{prompt_headline0}}</h2>
      <p class="view-promptset-starting-text">{{prompt0}}</p>
    </div>


<!-- Toggle Arrow for Expanding Main Section -->
<div class="view-promptset-main-toggle" onclick="togglePromptsetMain(this)" data-authorized="{{isAuthorizedToViewFullContent}}">
  <svg class="view-promptset-main-arrow" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 331.65 100.99">
    <defs><style>.cls-1 { fill: #4e50a2; }</style></defs>
    <g><polygon class="cls-1" points="0 0 161.57 100.99 331.65 0 0 0"/></g>
  </svg>
</div>
<div class="view-promptset-main-hidden-content">
  
  {{#if isAuthorizedToViewFullContent}}
    <!-- Tag Form -->
<!-- Tag Form (native POST, no AJAX) -->
<div class="view-promptset-tag-container">
  <form action="/tags/create" method="POST" class="inline">
    <input type="hidden" name="_csrf" value="{{csrfToken}}">
    <input type="hidden" name="itemId" value="{{_id}}">
    <input type="hidden" name="itemType" value="promptset">
    <input type="hidden" name="tagName" value="Read Later">
