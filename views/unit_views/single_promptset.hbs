<script>
function togglePromptsetMain(element) {
  const isAuthorized = element.dataset.authorized === "true";
  const hiddenContent = element.nextElementSibling;

  if (isAuthorized) {
    hiddenContent.classList.toggle("active");
    element.classList.toggle("open");
  } else {
    alert("You don’t have access to view this content.");
  }
}



</script>





<script>
function ensureDateBeforeSubmit() {
  const completionDateInput = document.getElementById('targetCompletionDate');
  const frequencyInput = document.getElementById('selectedFrequency');

  if (!frequencyInput.value) {
    alert("⚠️ Please select a frequency before assigning the prompt set.");
    return false;
  }
  if (!completionDateInput.value) {
    alert("⚠️ Target completion date is missing.");
    return false;
  }
  return true;
}

function selectFrequency(frequency, daysToAdd) {
    console.log("✅ Frequency clicked:", frequency);

    let frequencyInput = document.getElementById('selectedFrequency');
    let completionDateInput = document.getElementById('targetCompletionDate');

    frequencyInput.value = frequency;

    let currentDate = new Date();
    currentDate.setDate(currentDate.getDate() + daysToAdd);
    let formattedDate = currentDate.toISOString().split('T')[0];

    completionDateInput.value = formattedDate;
    console.log("✅ Updated Frequency:", frequencyInput.value);
    console.log("✅ Updated Completion Date:", completionDateInput.value);
}






function toggleMemberSelection(memberId, element) {
    let selectedMembersInput = document.getElementById('selectedMembers');
    let selectedMembers = selectedMembersInput.value ? selectedMembersInput.value.split(',') : [];
    
    if (selectedMembers.includes(memberId)) {
        selectedMembers = selectedMembers.filter(id => id !== memberId);
        element.style.color = "black";
        console.log(`❌ Unselected Member: ${memberId}`);
    } else {
        selectedMembers.push(memberId);
        element.style.color = "#ee2a7b";
        console.log(`✅ Selected Member: ${memberId}`);
    }
    
    selectedMembersInput.value = selectedMembers.join(',');
    console.log("Updated Selected Members:", selectedMembersInput.value);
}

</script>

<script>
function showSuccessMessage() {
    let assignedMembers = document.getElementById('selectedMembers').value.split(',').map(id => {
      let memberElement = document.querySelector(`.assign-member-option[onclick*='${id}'] span`);
      return memberElement ? memberElement.textContent : '';
    }).filter(name => name !== '').join(', ');
    
    document.getElementById('assignedMembersDisplay').textContent = assignedMembers;
    document.getElementById('assignSuccessMessage').classList.remove('hidden');
}

function validateAssignForm() {
    let frequency = document.getElementById('selectedFrequency').value;
    let completionDate = document.getElementById('targetCompletionDate').value;

    if (!frequency || !completionDate) {
        alert("⚠️ Please select a frequency before assigning the prompt set.");
        return false; // Prevent submission
    }

    console.log("✅ Form is valid. Submitting...");
    return true;
}




</script>



